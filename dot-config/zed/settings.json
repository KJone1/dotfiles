{
  "collaboration_panel": {
    "button": false,
  },
  "disable_ai": true,
  "session": {
    "trust_all_worktrees": true,
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "base_keymap": "VSCode",
  "autosave": {
    "after_delay": {
      "milliseconds": 1000,
    },
  },
  "auto_install_extensions": {
    "html": true,
    "dockerfile": true,
    "docker-compose": true,
    "toml": true,
    "xml": true,
    "lua": true,
    "just": true,
    "git-firefly": true,
    "fleet-themes": true,
  },
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "icon_theme": "Zed (Default)",
  "ui_font_size": 16,
  "buffer_font_size": 16.0,
  "theme": {
    "mode": "light",
    "light": "Fleet Light",
    "dark": "Ayu Dark",
  },
}
